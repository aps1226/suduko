"use strict";var _Asset = require("./Asset");
var _PlatformUtils = require("./PlatformUtils");
var _resolveAssetSource = require("./resolveAssetSource");

if (_PlatformUtils.IS_ENV_WITH_UPDATES_ENABLED) {
  (0, _resolveAssetSource.setCustomSourceTransformer)(function (resolver) {
    try {

      if (resolver.asset.fileHashes) {
        var asset = _Asset.Asset.fromMetadata(resolver.asset);
        return resolver.fromSource(asset.downloaded ? asset.localUri : asset.uri);
      } else
      {
        return resolver.defaultAsset();
      }
    }
    catch (e) {
      return resolver.defaultAsset();
    }
  });
}