var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _reactNative = require("react-native");

var _reactNative2 = require("@testing-library/react-native");

var _reactTestRenderer = _interopRequireDefault(require("react-test-renderer"));

var _types = require("../../../types");

var _index = require("../../state/reducers/index");

var _TextStroke = _interopRequireDefault(require("../../../components/TextStroke"));

describe('TextStroke', function () {
  it('renders without crashing', function () {
    var titleComponent = (0, _reactNative2.render)(_react.default.createElement(_TextStroke.default, {
      color: 'white',
      stroke: 2
    }, _react.default.createElement(_reactNative.Text, null, "Suduko"))).toJSON();
    expect(titleComponent).toMatchSnapshot();
  });
  it('renders with an invalid child element', function () {
    var titleComponent = (0, _reactNative2.render)(_react.default.createElement(_TextStroke.default, {
      color: 'white',
      stroke: 2
    }, "Suduko")).toJSON();
    expect(titleComponent.length).toBeTruthy();
  });
  it('renders correctly when the child property has specified style properties', function () {
    var titleComponent = (0, _reactNative2.render)(_react.default.createElement(_TextStroke.default, {
      color: 'white',
      stroke: 2
    }, _react.default.createElement(_reactNative.Text, {
      style: {
        color: 'black'
      }
    }, "Suduko"))).toJSON();
    expect(titleComponent.length).toBeTruthy();
  });
});
